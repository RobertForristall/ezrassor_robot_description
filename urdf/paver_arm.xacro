        <joint name="joint4" type="revolute">
            <origin xyz="0.42 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <parent link="link4"/>
            <child link="link5"/>
            <axis xyz="0.0 1 0.0"/>
            <dynamics damping="10.0" friction="10.0"/>
            <limit lower="-10.0" upper="10.0" effort="100" velocity="1.0"/>
        </joint>

        <joint name="joint5" type="revolute">
            <origin xyz="0.0 0.12 0.0" rpy="0.0 0.0 0.0"/>
            <parent link="link5"/>
            <child link="link6"/>
            <axis xyz="0.0 0.0 1"/>
            <dynamics damping="10.0" friction="10.0"/>
            <limit lower="-10.0" upper="10.0" effort="100" velocity="1.0"/>
        </joint>

        <joint name="grabber_joint1" type="prismatic">
            <origin xyz="0.1 -0.31 -0.24" rpy="0.0 0.0 0.0"/>
            <parent link="link6"/>
            <child link="grabber1"/>
            <axis xyz="1 1 0.0"/>
            <limit lower="-0.02" upper="0.0224" effort="10" velocity="0.005"/>
        </joint>

        <joint name="grabber_joint2" type="prismatic">
            <origin xyz="0.1 0.31 -0.24" rpy="0.0 0.0 0.0"/>
            <parent link="link6"/>
            <child link="grabber2"/>
            <axis xyz="1 -1 0.0"/>
            <limit lower="-0.02" upper="0.0224" effort="10" velocity="0.005"/>
        </joint>

