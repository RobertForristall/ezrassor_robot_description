                <origin xyz="0.23 0.0 0.0" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <xacro:link4/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0.23 0.0 0.0" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <xacro:link4/>
