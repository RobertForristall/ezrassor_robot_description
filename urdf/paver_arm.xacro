        <joint name="joint0" type="fixed">
            <xacro:if value='${model == "original"}'>
                <origin xyz="0.55 -0.1 0.27" rpy="0.0 0.0 0.0"/>
            </xacro:if>
            <xacro:if value='${model == "prototype"}'>
                <origin xyz="0.0 0.0 0.27" rpy="0.0 0.0 0.0"/>
            </xacro:if>
            <parent link="base_link"/>
            <child link="link1"/>
            <axis xyz="0.0 0.0 0.0"/>
        </joint>

        <joint name="joint1" type="revolute">
            <origin xyz="0.0 -0.12 0.09" rpy="0.0 0.0 0.0"/>
            <parent link="link1"/>
            <child link="link2"/>
            <axis xyz="0.0 0.0 1"/>
            <dynamics damping="10.0" friction="10.0"/>
            <limit lower="-10.0" upper="10.0" effort="100" velocity="1.0"/>
        </joint>

        <joint name="joint2" type="revolute">
            <origin xyz="0.0 0.037 0.13" rpy="0.0 0.0 0.0"/>
            <parent link="link2"/>
            <child link="link3"/>
            <axis xyz="0.0 1 0.0"/>
            <dynamics damping="10.0" friction="10.0"/>
            <limit lower="-10.0" upper="10.0" effort="100" velocity="1.0"/>
        </joint>

        <joint name="joint3" type="revolute">
            <origin xyz="0.44 -0.037 0.0" rpy="0.0 0.0 0.0"/>
            <parent link="link3"/>
            <child link="link4"/>
            <axis xyz="0.0 1 0.0"/>
            <dynamics damping="10.0" friction="10.0"/>
            <limit lower="-10.0" upper="10.0" effort="100" velocity="1.0"/>
